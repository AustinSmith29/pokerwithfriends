    "compilerOptions": {
        "target": "es5",
        "module": "commonJs",
        "sourceMap": true,
        "allowJs": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "./built"
    },
    "include": [ 
        "**/*.ts",
        "**/*.js"
    ]
}
